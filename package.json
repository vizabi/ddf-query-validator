{
  "name": "ddf-query-validator",
  "version": "1.0.3",
  "description": "DDF query validator",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run e2e",
    "test-travis": "npm run e2e && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "pree2e": "npm i",
    "e2e": "nyc mocha",
    "tslint": "./node_modules/.bin/tslint -c ./tslint.json 'src/**/*.ts'",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json && npm run tslint"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Oleksandra Kalinina<alexandra.kalinina@valor-software.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "lodash.clonedeep": "4.5.0",
    "lodash.compact": "3.0.1",
    "lodash.every": "4.6.0",
    "lodash.filter": "4.6.0",
    "lodash.first": "3.0.0",
    "lodash.flatmap": "4.5.0",
    "lodash.flattendeep": "4.4.0",
    "lodash.has": "4.5.2",
    "lodash.head": "4.0.1",
    "lodash.includes": "4.3.0",
    "lodash.isarray": "4.0.0",
    "lodash.isempty": "4.4.0",
    "lodash.isnil": "4.0.0",
    "lodash.isobject": "3.0.2",
    "lodash.isstring": "4.0.1",
    "lodash.keys": "4.2.0",
    "lodash.map": "4.6.0",
    "lodash.set": "4.3.2",
    "lodash.size": "4.2.0",
    "lodash.startswith": "4.2.1",
    "lodash.trimstart": "4.5.1",
    "lodash.uniq": "4.5.0",
    "lodash.values": "4.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/core-js": "2.5.0",
    "@types/lodash": "4.14.116",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.7",
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "sinon": "6.1.4",
    "source-map-support": "0.5.6",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  }
}
