{
  "name": "ddf-query-validator",
  "version": "1.0.0",
  "description": "DDF query validator",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run e2e",
    "test-travis": "npm run e2e && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "pree2e": "npm i",
    "e2e": "nyc mocha",
    "tslint": "./node_modules/.bin/tslint -c ./tslint.json 'src/**/*.ts'",
    "tsc": "./node_modules/.bin/tsc --project tsconfig.json && npm run tslint"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Oleksandra Kalinina<alexandra.kalinina@valor-software.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "lodash-es": "4.17.10"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/core-js": "2.5.0",
    "@types/lodash-es": "4.17.1",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.7",
    "chai": "4.1.2",
    "codecov": "3.0.4",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "sinon": "6.1.4",
    "source-map-support": "0.5.6",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  }
}
